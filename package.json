{
  "name": "@my-mcp-hub/node-mcp-client",
  "version": "0.0.0",
  "description": "MCP Client Template For Node.js",
  "author": "zhensherlock",
  "type": "module",
  "license": "MIT",
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "main": "build/index.js",
  "scripts": {
    "prepare": "husky",
    "lint": "npx eslint \"src/**/*.{ts,js}\"",
    "build": "tsc --noEmit && cross-env NODE_ENV=production node scripts/build.js",
    "dev": "cross-env NODE_ENV=local concurrently \"tsc --noEmit --watch\" \"node scripts/dev.js\"",
    "test": "vitest run",
    "coverage": "rimraf coverage && vitest run --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 -n changelog-option.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^4.0.1",
    "compare-func": "^2.0.0",
    "concurrently": "^9.2.1",
    "conventional-changelog-angular": "^8.1.0",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^10.1.0",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/my-mcp-hub/node-mcp-client.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
